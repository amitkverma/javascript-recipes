{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": [
            "./recipes/"
        ],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "docs"
    },
    "plugins": [],
    "opts": {
        "template": "templates/default",
        "encoding": "utf8",
        "destination": "./docs",
        "recurse": true,
        "private": false,
        "readme": "./readme.md"
    }
}